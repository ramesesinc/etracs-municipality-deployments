<schema>
	<element tablename="market_account_info">
		<field name="objid" primary="true" prefix="MKTACCTINFO"/>
		<field name="acctid" />
		<field name="acctname" />

		<field name="owner_objid" />
		<field name="owner_name" />
		<field name="owner_address_text" />
		<field name="owner_profileid" />
		<field name="owner_profileno" />
		<field name="owner_entitytype" required="true" />		
		<field name="owner_name" searchable="true" caption="Name"/>		
		<field name="owner_firstname" />
		<field name="owner_middlename" />
		<field name="owner_lastname" />
		<field name="owner_resident" />
		<field name="owner_address_objid" />
		<field name="owner_address_text" />
		<field name="owner_address_unitno" />
		<field name="owner_address_bldgno" />
		<field name="owner_address_bldgname" />
		<field name="owner_address_street" />
		<field name="owner_address_subdivision" />
		<field name="owner_address_barangay_objid" />
		<field name="owner_address_barangay_name" />
		<field name="owner_address_citymunicipality" />
		<field name="owner_address_province" />
		<field name="owner_tin" />
		<field name="owner_email" />
		<field name="owner_mobileno" />
		<field name="owner_phoneno" />
		<field name="owner_id_type_name" />
		<field name="owner_id_idno" />
		<field name="owner_id_placeissued" />
		<field name="owner_id_dtissued" />

		<field name="contact_mobileno" />
		<field name="contact_email" />
		<field name="contact_phoneno" />

		<field name="business_objid" />
		<field name="business_bin" />
		<field name="business_tradename" />

		<!-- link to current unit -->
		<field name="unitid" required="true"/>
		<field name="paymentmode" required="true"/>

		<!-- type of action that initiated the change info -->
		<field name="txnid" />
		<field name="txntype" />	

		<complex name="unit" ref="market_rental_unit" includefields="code,description,centerid" jointype="many-to-one" required="false">  
			<key field="unitid" target="objid" />
		</complex>
	</element>
</schema>



